<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondActivity">

    <RelativeLayout
        android:id="@+id/re3"
        android:layout_width="match_parent"
        android:layout_height="150dp">

        <TextView
            android:id="@+id/myname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="91dp"
            android:layout_marginLeft="91dp"
            android:layout_marginTop="26dp"
            android:text="Tajudeen Wariz"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/user"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/myname"
           android:layout_marginLeft="91dp"

            android:layout_marginTop="17dp"

            android:text="Bidex160"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/user"

            android:layout_marginTop="13dp"
       android:layout_marginLeft="91dp"
            android:text="thabidemi@gmail.com"
            android:textSize="20sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rel"

         android:id="@+id/re2"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="1dp">
        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"



            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"

            android:text="TextView"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_marginLeft="30dp"
            android:textSize="20sp"

            android:layout_marginTop="20dp"
            android:layout_toRightOf="@id/textView" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="30dp"
            android:layout_height="wrap_content"

            android:layout_marginLeft="36dp"
            android:layout_marginTop="22dp"

            android:layout_toRightOf="@id/textView2"
            android:text="1"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="TextView"

            android:layout_marginRight="20dp"
            android:textSize="20sp"
            android:layout_marginTop="20dp"
            android:layout_alignParentRight="true" />

        <TextView
            android:id="@+id/to"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textView4"
            android:layout_marginTop="20dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            android:text="TextView"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Total"
            android:textStyle="bold"
            android:textSize="20sp"
          android:layout_below="@id/textView2"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="23dp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rel"
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/re3"

        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="5dp">

        <TextView
            android:id="@+id/text"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="7dp"
            android:layout_marginLeft="7dp"
            android:layout_marginTop="20dp"
            android:text="List Items"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/Prce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="17dp"
            android:layout_marginLeft="17dp"
            android:layout_marginTop="24dp"
            android:layout_toEndOf="@+id/text"
            android:layout_toRightOf="@+id/text"
            android:text="Price"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/Qttt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginStart="52dp"
            android:layout_marginLeft="52dp"
            android:layout_marginTop="20dp"

            android:layout_toEndOf="@+id/Prce"
            android:layout_toRightOf="@+id/Prce"
            android:text="Qt"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="51dp"
            android:layout_marginTop="20dp"
            android:layout_toRightOf="@+id/Qttt"
            android:text="Total"
            android:textSize="20sp" />




    </RelativeLayout>
    <Button
        android:id="@+id/sharbt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/re2"
        android:layout_marginTop="78dp"
        android:text="Share"
        android:layout_centerHorizontal="true"/>


</RelativeLayout>